<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Default Unit Testing Suite" parallel="methods">
    <listeners>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener" />
    </listeners>
    <test name="Calculator Tests" thread-count="5">
        <groups>
            <run>
                <include name = "positive" />
                <include name = "negative" />
            </run>
        </groups>
        <classes>
            <class name="com.cnu.mentoring.vineet.cdp.unittesting.CalculatorHomeTaskTests" />
        </classes>
    </test>
</suite>